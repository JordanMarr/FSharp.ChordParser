            "preLaunchTask": "build",
            "program": "${workspaceFolder}/ChordParser.UI/bin/Debug/net6.0/ChordParser.UI.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,

            "env": {
                "LANG": "en_US.UTF-8",
                "FUNCUI_LIVEPREVIEW": "1"
            },
            "linux": {
                "env": {
                    "LANG": "en_US.UTF-8",
                    "FUNCUI_LIVEPREVIEW": "1"
                }
